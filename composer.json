{
    "name": "arrayiterator/profiler",
    "description": "Record & profiling the code",
    "keywords": [
        "benchmark",
        "profiler",
        "php profiler",
        "record"
    ],
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "ArrayIterator",
            "email": "me@arrayiterator.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.10.3",
        "slevomat/coding-standard": "^8.15.0",
        "phpstan/phpstan": "^1.12",
        "psr/http-message": "^1.0.1"
    },
    "config": {
        "vendor-dir": "vendor",
        "optimize-autoloader": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "ArrayIterator\\Profiler\\": "src/"
        }
    }
}
